             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpCommunityToolkitMVVM"
             xmlns:cv="clr-namespace:WpCommunityToolkitMVVM.Converters">
